# ðŸ§¹ Datenbereinigung: Fehlwerte (-99) als NA setzen
data_clean <- data %>% mutate(across(where(is.numeric), ~ na_if(., -99)))
# ðŸ§® Ãœberblick Ã¼ber zentrale NÃ¤hrstoffe
nutrients <- c("ZE", "ZF", "ZK", "GJ", "FC", "ZB", "MFE", "VB12")
# ðŸ“Š Deskriptive Statistiken nach Gruppe
summary_stats <- data_clean %>%
group_by(group) %>%
summarise(across(all_of(nutrients), list(mean = ~mean(., na.rm = TRUE),
